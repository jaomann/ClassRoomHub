@using ClassroomHub.Core.Entities;
@model IEnumerable<User>;
<table class="table table-striped table-hover text-center table-dark">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nome</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
        <tr>
            <td>
                @user.Id;
            </td>
            <td>
                @user.Email;
            </td>
        </tr>
        }
    </tbody>
</table>